echo "Line "$1
# Set Default values
GuitarEffects=true
EffectsProcessorName="Guitarix"
EffectsProcessorApp="guitarixOld"
EffectsProcessorWM="Guitarix: gx_head"
# PatchBay="carla"
PatchBay="qpwgraph"
